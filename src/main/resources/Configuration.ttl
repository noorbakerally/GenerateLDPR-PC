@base <http://localhost:8888/> .
@prefix ldp:     <http://www.w3.org/ns/ldp#>.
@prefix :     <http://opensensingcity.emse.fr/LDPDesignVocabulary/> .
@prefix data:     <http://opensensingcity.emse.fr/LDPDesign/data/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

data:CatalogMap a :ContainerMap;
	:containerMap data:ContainerMapDataset;
	:containerType ldp:BasicContainer;
	:resourceMap data:CatalogResourceMap .

data:CatalogResourceMap a :ResourceMap;
    :resourceQuery "Select * WHERE { ?x a dcat:Catalog .}";
    :graphTemplate "CONSTRUCT { ?_resource ?p ?o . } WHERE {  ?_resource ?p ?o . FILTER (?p not in (dcat:dataset)) }";
    :linkToSource foaf:primaryTopic;
    :dataSource data:DataSource1 .

data:ContainerMapDataset a :ContainerMap;
	:resourceMap data:DataSetResourceMap;
	:containerMap data:ContainerMapDistribution .

data:DataSetResourceMap a :ResourceMap;
    :resourceQuery "Select * WHERE { ?_resource dcat:dataset  ?x . } LIMIT 5";
    :graphTemplate "CONSTRUCT { ?_resource ?p ?o . } WHERE {  ?_resource ?p ?o . FILTER (?p not in (dcat:distribution)) }";
    :linkToSource foaf:primaryTopic;
    :dataSource data:DataSource1 .


data:ContainerMapDistribution a :ContainerMap;
    :resourceMap data:DistributionResourceMap .

data:DistributionResourceMap a :ResourceMap;
    :resourceQuery "Select * WHERE { ?_resource dcat:distribution  ?x . } LIMIT 5";
    :graphTemplate "CONSTRUCT { ?_resource ?p ?o . } WHERE {  ?_resource ?p ?o . }";
    :linkToSource foaf:primaryTopic;
    :dataSource data:DataSource1 .

data:DataSource1 a :DataSource;
    :location "/home/noor/Downloads/paris.ttl" .